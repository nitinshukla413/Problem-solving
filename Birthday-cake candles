
#include <bits/stdc++.h>

using namespace std;

int main()
{
    int n;
    cin>>n;
long int candles[n]; 
long int max = 0; 
long int count = 0;

for(int i = 0; i < n; i++){
   cin >> candles[i];
    if(candles[i] > max)
        max = candles[i];
}


for(int i = 0; i < n; i++){
    if(candles[i] == max)
        count++;
}


cout << count;

return 0;
}
