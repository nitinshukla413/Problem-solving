#include <bits/stdc++.h>

using namespace std;

int main()
{
    
    long int n,min=0,max=0;
    
    vector<long int> v;
    for(int i=0;i<5;i++)
    {
        cin>>n;
        v.push_back(n);
       
    }
    sort(v.begin(),v.end());
    
    for(int i=0;i<4;i++)
    {
        min+=v[i];
    }
    for(int i=1;i<5;i++)
    {
        max+=v[i];
    }
    cout<<min<<" "<<max<<endl;
   
   
}
